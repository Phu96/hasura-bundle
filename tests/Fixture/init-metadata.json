{
  "type": "replace_metadata",
  "version": 2,
  "args": {
    "allow_inconsistency": false,
    "metadata": {
      "version": 3,
      "sources": [
        {
          "name": "default",
          "kind": "postgres",
          "tables": [
            {
              "table": {
                "schema": "public",
                "name": "product_users"
              },
              "object_relationships": [
                {
                  "name": "product",
                  "using": {
                    "foreign_key_constraint_on": "product_id"
                  }
                },
                {
                  "name": "user",
                  "using": {
                    "foreign_key_constraint_on": "user_id"
                  }
                }
              ]
            },
            {
              "table": {
                "schema": "public",
                "name": "products"
              },
              "array_relationships": [
                {
                  "name": "product_users",
                  "using": {
                    "foreign_key_constraint_on": {
                      "column": "product_id",
                      "table": {
                        "schema": "public",
                        "name": "product_users"
                      }
                    }
                  }
                }
              ]
            },
            {
              "table": {
                "schema": "public",
                "name": "users"
              },
              "array_relationships": [
                {
                  "name": "product_users",
                  "using": {
                    "foreign_key_constraint_on": {
                      "column": "user_id",
                      "table": {
                        "schema": "public",
                        "name": "product_users"
                      }
                    }
                  }
                }
              ],
              "event_triggers": [
                {
                  "name": "insertedUser",
                  "definition": {
                    "enable_manual": false,
                    "insert": {
                      "columns": "*"
                    }
                  },
                  "retry_conf": {
                    "num_retries": 0,
                    "interval_sec": 10,
                    "timeout_sec": 60
                  },
                  "webhook": "http://localhost/hasura_event"
                }
              ]
            }
          ],
          "configuration": {
            "connection_info": {
              "use_prepared_statements": true,
              "database_url": {
                "from_env": "HASURA_GRAPHQL_DATABASE_URL"
              },
              "isolation_level": "read-committed",
              "pool_settings": {
                "connection_lifetime": 600,
                "retries": 1,
                "idle_timeout": 180,
                "max_connections": 50
              }
            }
          }
        }
      ],
      "query_collections": [
        {
          "name": "allowed-queries",
          "definition": {
            "queries": [
              {
                "name": "Get Users",
                "query": "query GetUsers {\n  users {\n    id \n    name\n  }\n}"
              },
              {
                "name": "Get Products",
                "query": "query GetProducts {\n  products {\n    id\n    name\n  }\n}"
              }
            ]
          }
        }
      ],
      "allowlist": [
        {
          "collection": "allowed-queries"
        }
      ],
      "actions": [
        {
          "name": "insertProduct",
          "definition": {
            "handler": "http://localhost/hasura_action",
            "output_type": "InsertProductOutput",
            "arguments": [
              {
                "name": "name",
                "type": "String!"
              }
            ],
            "type": "mutation",
            "kind": "synchronous"
          }
        }
      ],
      "custom_types": {
        "objects": [
          {
            "name": "InsertProductOutput",
            "fields": [
              {
                "name": "id",
                "type": "uuid!"
              }
            ]
          }
        ]
      },
      "rest_endpoints": [
        {
          "definition": {
            "query": {
              "collection_name": "allowed-queries",
              "query_name": "Get Products"
            }
          },
          "url": "products",
          "methods": [
            "GET"
          ],
          "name": "Get Products",
          "comment": null
        },
        {
          "definition": {
            "query": {
              "collection_name": "allowed-queries",
              "query_name": "Get Users"
            }
          },
          "url": "users",
          "methods": [
            "GET"
          ],
          "name": "Get Users",
          "comment": null
        }
      ]
    }
  }
}